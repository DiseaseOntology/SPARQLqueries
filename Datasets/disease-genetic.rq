#id: DOq063
#description: >-
#  All diseases 'genetic disease' and under
#input: doid.owl
#comment: Created for the International Committee on Taxonomy of Viruses (ICTV)
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX oboInOwl: <http://www.geneontology.org/formats/oboInOwl#>
PREFIX DOID: <http://purl.obolibrary.org/obo/DOID_>
PREFIX NCBITaxon: <http://purl.obolibrary.org/obo/NCBITaxon_>
PREFIX IAO: <http://purl.obolibrary.org/obo/IAO_>

SELECT ?iri ?id ?label ?definition
FROM <http://purl.obolibrary.org/obo/doid/doid-merged.owl>
WHERE {
  ?iri a owl:Class ;
    oboInOwl:id ?id ;
    rdfs:label ?label ;
    rdfs:subClassOf* DOID:630 .

  OPTIONAL { ?iri IAO:0000115 ?definition }
}