###  http://www.example.org/class1
:class1 rdf:type owl:Class ;
        rdfs:subClassOf :parent1 ,
                        :parent2 ;
        rdfs:comment "closest common ancestor with class2 = parent1 & parent2 (1)" .


###  http://www.example.org/class2
:class2 rdf:type owl:Class ;
        rdfs:subClassOf :parent1 ,
                        :parent2 .


###  http://www.example.org/class3
:class3 rdf:type owl:Class ;
        rdfs:subClassOf :parent1 ,
                        :parent3 ;
        rdfs:comment """closest common ancestor with class1/2 = parent1 (1)
closest common ancestor with class4 = grandparent1 & grandparent2 (2)""" .


###  http://www.example.org/class4
:class4 rdf:type owl:Class ;
        rdfs:subClassOf :parent2 ,
                        :parent4 .


###  http://www.example.org/grandparent1
